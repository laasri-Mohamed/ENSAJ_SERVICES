            <div class="container-fluid">
                <!-- ============================================================== -->
                <!-- Start Page Content -->
                <!-- ============================================================== -->
                <div class="row">
                    <!-- column -->
                    <% modules.forEach(module=> { %>
                        <section id="mysection">
                            <div class="col-sm-12">
                                <div class="card">
                                    <div class="card-body">
                                        <h4 class="card-title">
                                            <%= module.classe %> - <%= module.nom %>
                                        </h4>
                                        <h6 class="card-subtitle">Liste des étudiants<code> </code> </h6>
                                        <table class="table user-table">
                                            <thead>
                                                <tr>
                                                    <th class="border-top-0">Image</th>
                                                    <th class="border-top-0">Nom complet</th>
                                                    <th class="border-top-0">Note</th>
                                                    <th class="border-top-0">Action</th>
                                                </tr>
                                            </thead>

                                            <tbody>
                                                <% person.forEach(student=> { %>
                                                    <% if (module.classe.includes(student.filiere)) { %>
                                                        <tr>
                                                            <td>
                                                                <img src="static/uploads/images/<%= student.image %>"
                                                                    class="rounded-circle" alt="Avatar"
                                                                    style="width: 50px; height: 50px;">
                                                            </td>
                                                            <td>
                                                                <%= student.prenom + " " + student.nom %>
                                                            </td>

                                                            <% let studentNotes=notes.filter(mark=> mark.module ===
                                                                module.nom && mark.etudiant === (student.prenom + " " +
                                                                student.nom)); %>

                                                                <% if (studentNotes.length> 0) { %>
                                                                    <form action="./updatemark" method="post">
                                                                        <td><strong>
                                                                                <%= studentNotes[0].note %>
                                                                            </strong></td>
                                                                        <input type="hidden" name="studentid"
                                                                            value="<%= student._id %>">
                                                                        <input type="hidden" name="note"
                                                                            value="<%= studentNotes[0].note %>">
                                                                        <input type="hidden" name="nomModule"
                                                                            value="<%= module.nom %>">
                                                                        <td><button
                                                                                class="btn btn-outline-secondary">Modifier
                                                                                une note</button></td>
                                                                    </form>
                                                                    <% } else { %>
                                                                        <td>
                                                                            <form action="./addmark" method="post">
                                                                                <input type="hidden" name="studentid"
                                                                                    value="<%= student._id %>">
                                                                                <input type="hidden" name="nomModule"
                                                                                    value="<%= module.nom %>">
                                                                                <button
                                                                                    class="btn btn-outline-primary">Attribuer
                                                                                    une note</button>
                                                                            </form>
                                                                        </td>
                                                                        <form action="./updatemark" method="post">
                                                                            <input type="hidden" name="studentid"
                                                                                value="<%= student._id %>">
                                                                            <td><button
                                                                                    class="btn btn-outline-secondary"
                                                                                    disabled>Modifier une note</button>
                                                                            </td>
                                                                        </form>
                                                                        <% } %>

                                                        </tr>
                                                        <% } %>
                                                            <% }) %>
                                            </tbody>

                                        </table>
                                    </div>
                                </div>
                            </div>
                        </section>
                        <br />
                        <button class="btn btn-outline-primary"
                            onclick="toggle(this, 'mysection', '<%=module.nom%>' )">▷ Masquer la liste - <%= module.nom
                                %> </button>
                        <div><br></div>
                        <% }) %>




