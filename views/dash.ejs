                <div>
                    <canvas id="doughnut"></canvas>
                </div>
            </div>
            <div class="chart">
                <h2>La moyenne des notes par classe</h2>
                <div>
                    <canvas id="barchart"></canvas>
                </div>
            </div>
            <div class="chart pie-chart">
                <h2>Etudiants</h2>
                <div>
                    <canvas id="piechart"></canvas>
                </div>
            </div>
        </div>
    </div>
    </div>
